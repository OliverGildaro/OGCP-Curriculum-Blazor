@page "/"
@rendermode InteractiveServer

<div class="container" style="display:flex; padding-top:20px;">
    <h4>Profiles</h4>
    @foreach (var Current in profiles)
    {
        <div class="col">
            <span>@Current.GivenName</span>
        </div>
    }
</div>
<ButtonCmp
    CssClass="btn-primary" />
<InputField
    Label="etiqueta"
    Placeholder="Name"
    Value=""
    FormClass="is-valid"
    Feedback="Error here"
    FieldType="text"
    FeedbackClass="valid-feedback" />